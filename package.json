{
    "name": "pal.video",
    "version": "1.0.4",
    "description": "Your in-app friend.",
    "main": "build/src/pal.js",
    "cdn": "build/pal.min.js",
    "module": "build/src/index.es.js",
    "types": "build/src/index.d.ts",
    "files": [
        "/build/src",
        "/build/pal.min.js",
        "/build/style.css"
    ],
    "author": {
        "name": "Pal team",
        "web": "https://pal.video"
    },
    "keywords": [
        "pal",
        "feedbacks",
        "video"
    ],
    "scripts": {
        "lint": "gts lint",
        "clean": "gts clean",
        "compile": "rollup -c rollup.config.js && cp src/sdk/style.css build/style.css",
        "compile-ts": "npm run clean && tsc --project tsconfig.json",
        "fix": "gts fix",
        "prepare": "npm run compile",
        "pretest": "npm run compile",
        "posttest": "npm run lint",
        "test": "jest",
        "test-watch": "jest --watchAll",
        "publish-npm": "npm run compile-ts && npm run compile && npm publish"
    },
    "dependencies": {},
    "devDependencies": {
        "@babel/core": "^7.18.5",
        "@babel/preset-env": "^7.18.2",
        "@rollup/plugin-babel": "^5.3.1",
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-eslint": "^8.0.2",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@rollup/plugin-typescript": "^8.3.3",
        "@types/jest": "^28.1.1",
        "@types/node": "^14.11.2",
        "babel-preset-es2015": "^6.24.1",
        "gts": "^3.1.0",
        "jest": "^28.1.1",
        "jest-environment-jsdom": "^28.1.1",
        "mock-local-storage": "^1.1.23",
        "rollup": "^2.75.6",
        "rollup-plugin-node-builtins": "^2.1.2",
        "rollup-plugin-node-globals": "^1.4.0",
        "rollup-plugin-terser": "^7.0.2",
        "ts-jest": "^28.0.5",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.3"
    }
}